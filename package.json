      "quality": {
        "enabled": true,
        "severity": "warning",
        "autoFix": true
      },
      "multitenant": {
        "enabled": true,
        "severity": "error",
        "autoFix": false
      },
      "performance": {
        "enabled": true,
        "severity": "warning",
        "autoFix": false
      }
    },
    "reporting": {
      "format": "console",
      "outputFile": "governance-report.json",
      "includeWarnings": true,
      "verboseMode": false
    },
    "enforcement": {
      "blockCommitOnFailure": true,
      "blockDeployOnFailure": true,
      "allowWarnings": true
    },
    "exemptions": {
      "files": [
        "*.test.ts",
        "*.spec.ts",
        "tools/",
        "tests/"
      ],
      "rules": {
        "legacy": [
          "quality.documentation"
        ]
      }
    }
  }
}
