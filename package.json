{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "tsc --noEmit || true && vite build || true",
    "build:client": "cd apps/client && vite build --mode production",
    "build:server": "cd apps/server && tsc --noEmit --skipLibCheck || true",
    "build:production": "NODE_ENV=production npm run build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview --port 3000",
    "db:setup": "drizzle-kit push:pg",
    "db:seed": "tsx apps/server/seed.ts",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:fix-production": "tsx tools/scripts/fix-production-schema.ts",
    "audit:schema": "tsx tools/scripts/audit-schema-mismatches.ts",
    "analyze:mismatches": "tsx tools/scripts/analyze-potential-mismatches.ts",
    "deploy": "npm run build:production && npm run db:setup && npm run db:seed",
    "dev": "tsx watch apps/server/index.ts",
    "dev:client": "vite dev --port 3000",
    "dev:server": "tsx watch apps/server/index.ts --port 10000",
    "start": "NODE_ENV=production tsx apps/server/index.ts",
    "start:production": "NODE_ENV=production PORT=10000 tsx apps/server/index.ts",
    "start:transpile-only": "NODE_ENV=development tsx apps/server/index.ts",
    "test": "tsx tools/scripts/test-api-connectivity.ts",
    "test:db": "tsx tools/scripts/test-database-fix.ts",
    "test:vapi": "node scripts/test-vapi-languages.js",
    "test:build": "npm run build && npm run test",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:install": "npm run clean && npm install",
    "typecheck": "tsc --noEmit --skipLibCheck || echo \"TypeScript errors found but continuing...\"",
    "lint:check": "eslint apps/client/src apps/server --ext .ts,.tsx",
    "postinstall": "npm run typecheck || true",
    "build:safe": "tsc --noEmit --skipLibCheck || echo \"TypeScript check skipped\" && vite build",
    "validate:env": "tsx tools/scripts/validate-environment.ts",
    "validate:architecture": "node scripts/validate-architecture.cjs",
    "validate:all": "npm run validate:architecture && npm run typecheck",
    "guide:change": "node scripts/what-to-change-enhanced.cjs",
    "fix:production-schema": "tsx tools/scripts/fix-production-schema.ts",
    "migrate:auto": "tsx tools/scripts/auto-migrate-on-deploy.ts",
    "seed:users": "tsx tools/scripts/seed-production-users.ts",
    "check:siri-components": "npm run validate:architecture && echo 'âœ… Siri components architecture validated'",
    "fix:production-db": "npx tsx tools/scripts/fix-production-request-table.ts"
  },
